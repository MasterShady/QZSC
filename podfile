platform :ios, '14.0'

source 'https://github.com/CocoaPods/Specs.git'

source 'http://git.zhw.com/mengke/Specs.git'

#use_frameworks!
use_modular_headers!
# 忽略第三方库的警告
inhibit_all_warnings!

target 'QZSC' do

  pod 'SnapKit', '~> 5.6.0'
  pod 'RxSwift', '~> 6.5.0'
  pod 'RxCocoa', '~> 6.5.0'
  pod 'Cache', '~> 6.0.0'
  pod 'MJRefresh', '~> 3.7.5'
  pod 'Kingfisher', '~> 7.8.1'
  pod 'Alamofire','~> 5.2.0'
  pod 'IQKeyboardManagerSwift', '6.3.0'
  pod 'MBProgressHUD', '1.1.0'
  pod 'CLImagePickerTool'
  pod 'JXPagingView', '~> 2.1.2'
  pod 'JXSegmentedView', '~> 1.3.0'
  pod 'ActiveLabel', '~> 1.1.0'
  pod 'SwiftUIIntrospect'
  #pod 'LookinServer', :configurations => ['Debug']
  pod 'Masonry', '~> 1.1.0'
  pod 'AFNetworking', '~> 4.0.1'
  pod 'YYModel', '~> 1.0.4'
  #pod 'YYKit'

end


post_install do |installer|
  installer.generated_projects.each do |target|
      target.build_configurations.each do |config|
        config.build_settings['CODE_SIGNING_ALLOWED'] = "NO"
        config.build_settings['CODE_SIGNING_REQUIRED'] = "NO"
        config.build_settings['EXPANDED_CODE_SIGN_IDENTITY'] = ""
      end
    end
end
